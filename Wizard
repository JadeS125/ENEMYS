public class Wizard extends Enemy{
    String type;
    public Wizard(int h, int d, String t) {
        super(h, d);
        type = t;
    }
    void damageType(){

        if(type.equals("fire")) {
            System.out.println(" This wizard shoots a fireball.");
        } else {
        System.out.println(" This wizard shoots an iceball.");
        }
    }
}
